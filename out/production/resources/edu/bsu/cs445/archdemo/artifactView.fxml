<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<fx:root maxHeight="500" maxWidth="500" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
    <Label fx:id="titleView" text="null" wrapText="true">
      <padding>
         <Insets bottom="15.0" top="15.0" />
      </padding></Label>
    <Label fx:id="artistView" text="null" wrapText="true">
      <padding>
         <Insets bottom="15.0" />
      </padding></Label>
    <Label fx:id="subjectView" text="null" wrapText="true">
      <padding>
         <Insets bottom="15.0" />
      </padding></Label>
    <Label fx:id="dateView" text="null" wrapText="true">
      <padding>
         <Insets bottom="15.0" />
      </padding></Label>
    <ImageView fx:id="imageView" fitHeight="300.0" fitWidth="300.0" preserveRatio="true" />
</fx:root>
